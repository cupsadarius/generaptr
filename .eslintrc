{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
  },

  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "forOf": true,
    "superInFunctions": true
  },

  "rules": {
    "arrow-spacing": 2,
    "accessor-pairs": 1,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "comma-dangle": [2, "always-multiline"],
    "complexity": [1, 10],
    "consistent-this": [2, "self"],
    "constructor-super": 2,
    "curly": 2,
    "default-case": 2,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 1,
    "indent": [2, 2, {"SwitchCase": 1}],
    "jsx-quotes": [2, "prefer-double"],
    "linebreak-style": [2, "unix"],
    "new-parens": 2,
    "new-cap": 1,
    "no-alert": 2,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-div-regex": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [1, "functions"],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-func-assign": 2,
    "no-implicit-coercion": 1,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-loop-func": 2,
    "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 1}],
    "no-multi-spaces": 2,
    "no-multi-str": 1,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 1,
    "no-process-env": 2,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-regex-spaces": 2,
    "no-restricted-syntax": [2, "WithStatement"],
    "no-return-assign": 1,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, {"args": "none"}],
    "no-use-before-define": 2,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-var": 2,
    "no-void": 2,
    "no-with": 2,
    "padded-blocks": [2, "never"],
    "prefer-arrow-callback": 2,
    "prefer-spread": 2,
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "always"],
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-keywords": 0,
    "space-in-parens": 2,
    "space-infix-ops": [2],
    "strict": [2, "never"],
    "use-isnan": 2,
    "valid-jsdoc": [1, { "requireReturn": false }],
    "valid-typeof": 2,
    "yoda": 2
  }
}