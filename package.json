{
  "name": "generaptr",
  "scripts": {
    "compile": "npm-run-all -p compile:core",
    "compile:core": "tsc -p src",
    "lint": "tslint --project ./src/tsconfig.json --format stylish",
    "test": "./node_modules/.bin/mocha test/**/*Spec.js test/**/**/*Spec.js",
    "test:coverage": "export NODE_ENV='test' && node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports test/**/*Spec.js test/**/**/*Spec.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "logs": "tail -f debug-log.json | ./node_modules/.bin/bunyan"
  },
  "version": "0.9.0",
  "description": "Api generator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cupsadarius/generaptr.git"
  },
  "author": "Darius Cupsa <cupsadarius@gmail.com>",
  "main": "./lib/generaptr.js",
  "bugs": {
    "url": "https://github.com/cupsadarius/generaptr/issues"
  },
  "contributors": [
    "Boieru Luca <luca.boieru@yahoo.com>",
    "Balea Stefan <stefan.balea16@gmail.com>"
  ],
  "keywords": [
    "nodejs"
  ],
  "bin": {
    "generaptr": "./bin/generaptr"
  },
  "engines": {
    "node": ">= 7.5.0",
    "typescript": ">= 2.4.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "coveralls": "^2.13.0",
    "istanbul": "^0.4",
    "mocha": "^3.4.0"
  },
  "dependencies": {
    "@types/chalk": "^0.4.31",
    "@types/inquirer": "0.0.35",
    "bunyan": "^1.8.9",
    "chalk": "^2.0.1",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "faker": "^4.1.0",
    "figlet": "^1.2.0",
    "fs-extra": "^4.0.0",
    "get-env": "^0.5.10",
    "inquirer": "^3.0.6",
    "minimist": "^1.2.0",
    "mysql": "^2.13.0",
    "npm-run-all": "^4.0.2",
    "pluralize": "^6.0.0",
    "preferences": "^0.2.1",
    "raml-1-parser": "^1.1.26",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
