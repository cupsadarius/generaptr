{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "declaration": true,
    "sourceMap": false,
    "target": "es5",
    "lib": ["es6"],
    "outDir": "../lib",
    "typeRoots": ["../typings/globals", "../typings/modules"]
  }
}